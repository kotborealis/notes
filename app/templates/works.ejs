<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8" />
  <title>Works</title>

  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <meta http-equiv="X-UA-Compatible" content="ie=edge">

  <link rel="shortcut icon" href="./assets/favicon.ico" type="image/x-icon">
  <link rel="preload" href="./styles/works.css" as="style">
  <link rel="stylesheet" href="./styles/works.css">
</head>
<body>
  <header id="top-container" role="navigation">
    <%- include(process.cwd() + '/app/templates/navigation'); %>
  </header>
  <main id="main-container">
    <section id="works-container">
      <% works.reverse().map((work) => { %>
      <a href="./work/<%= work.id %>.html">
        <div class="works-content">
          <img src="<%= work.thumbnail %>" />
          <h1><%= work.title %></h1>
        </div>
      </a>
      <% }) %>
    </section>
  </main>
</body>
</html>
